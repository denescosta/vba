    Dim Arquivo As Variant
    Dim wbOrigem As Workbook
    Dim wsOrigem As Worksheet
    Dim wsDestino As Worksheet
    Dim LastRowOrigem As Long
    Dim LastRowDestino As Long
    Dim LastRowDestinoC As Long ' Última linha na coluna C do destino
    Dim LastColumnOrigem As Long
    Dim LastColumnDestino As Long
    
    ' Mostra o diálogo de seleção de arquivo
    Arquivo = Application.GetOpenFilename("Arquivos Excel (*.xlsx), *.xlsx", , "Selecione o arquivo de origem")
    
    ' Verifica se um arquivo foi selecionado
    If Arquivo <> False Then
        ' Abre o arquivo selecionado
        Set wbOrigem = Workbooks.Open(Arquivo)
        
        ' Defina a primeira planilha do arquivo de origem como a planilha de origem
        Set wsOrigem = wbOrigem.Sheets(1)
        
        ' Defina a primeira planilha do arquivo atual como a planilha de destino
        Set wsDestino = ThisWorkbook.Sheets(1)
        
        ' Encontra a última linha na coluna E da planilha de origem
        LastRowOrigem = wsOrigem.Cells(wsOrigem.Rows.Count, "E").End(xlUp).Row
        
        ' Encontra a última linha na coluna A da planilha de destino
        LastRowDestino = wsDestino.Cells(wsDestino.Rows.Count, "A").End(xlUp).Row
        
        ' Encontra a última linha na coluna C da planilha de destino
        LastRowDestinoC = lastRow(wsDestino, "C")
        
        ' Encontra a última coluna na planilha de origem
        LastColumnOrigem = wsOrigem.Cells(1, wsOrigem.Columns.Count).End(xlToLeft).Column
        
        ' Encontra a última coluna na planilha de destino
        LastColumnDestino = wsDestino.Cells(1, wsDestino.Columns.Count).End(xlToLeft).Column
        
        ' Copia os dados das colunas A, B e D do arquivo de origem para o arquivo atual
        wsOrigem.Range("A1:B" & LastRowOrigem).Copy Destination:=wsDestino.Range("A" & LastRowDestino)
        wsOrigem.Range("D1:D" & LastRowOrigem).Copy Destination:=wsDestino.Range("C1:C" & LastRowDestino + LastRowOrigem)
        
        ' Copia apenas os valores da célula da última linha da coluna E do arquivo de origem para a célula correspondente na coluna C do arquivo de destino
        wsDestino.Cells(LastRowOrigem, "C").Value = wsOrigem.Cells(LastRowOrigem, "E").Value
        
        ' Deleta a penúltima linha inteira do destino
        wsDestino.Rows(LastRowOrigem - 1).EntireRow.Delete
        
        ' Limpa a área de transferência
        Application.CutCopyMode = False
        
        ' Feche o arquivo de origem sem salvar alterações
        wbOrigem.Close SaveChanges:=False
        
        MsgBox "Dados copiados e última linha acrescentada com sucesso!"
    Else
        MsgBox "Nenhum arquivo selecionado."
    End If
End Sub

Function lastRow(ws As Worksheet, col As String) As Long
    lastRow = ws.Cells(ws.Rows.Count, col).End(xlUp).Row
    If lastRow = 1 And ws.Cells(1, col).Value = "" Then lastRow = 0 ' Caso a coluna esteja vazia
End Function


Sub aaaCopiarLayoutOrçamentos()
    Dim wbOrigem As Workbook
    Dim wsOrigem As Worksheet
    
    ' Abra o arquivo de origem
    Set wbOrigem = Workbooks.Open("Z:\Engenharia\layout-orçamentos.xlsx") ' Substitua pelo caminho do arquivo de origem
    
    ' Defina a planilha de origem
    Set wsOrigem = wbOrigem.Sheets("orçamentos") ' Substitua "PlanilhaOrigem" pelo nome da planilha de origem
    
    ' Copie a planilha de origem para a planilha atual
    Application.DisplayAlerts = False ' Desativa as mensagens de alerta para substituir a planilha
    wsOrigem.Copy Before:=ThisWorkbook.Sheets(1) ' Substitua pelo índice da planilha que deseja substituir
    Application.DisplayAlerts = True ' Ativa novamente as mensagens de alerta
    
    ' Exclui a planilha original
    wbOrigem.Close SaveChanges:=False ' Fecha o arquivo de origem sem salvar alterações
    
    ' Renomeie a nova planilha, se necessário
    ThisWorkbook.Sheets(1).Name = "Custos" ' Substitua "PlanilhaSubstituida" pelo nome desejado
    
End Sub


Sub LerValoresDoArquivoDeReferencia(ByRef valoresB() As Variant, ByRef valoresE() As Variant)
    Dim outroArquivo As String
    Dim wb As Workbook
    Dim ws As Worksheet
    Dim lastRow As Long
    Dim i As Long
    
    ' Defina o caminho do arquivo externo
    outroArquivo = "Z:\Engenharia\Custos e Pré-projetos\2023\LISTA DE PREÇOS\Lista Geral de Preços Fluid Feeder - 2º semestre 2023 (Engenharia).xlsx"
    
    ' Verifique se o arquivo existe
    If Dir(outroArquivo) = "" Then
        MsgBox "O arquivo não foi encontrado."
        Exit Sub
    End If
    
    ' Abra o arquivo externo
    Set wb = Workbooks.Open(outroArquivo, ReadOnly:=True)
    
    ' Verifique se a planilha existe
    On Error Resume Next
    Set ws = wb.Sheets("Lista de preços")
    On Error GoTo 0
    
    If ws Is Nothing Then
        MsgBox "A planilha especificada não foi encontrada no arquivo."
        wb.Close False
        Exit Sub
    End If
    
    ' Encontre a última linha com dados na coluna B e na coluna E
    lastRow = ws.Cells(ws.Rows.Count, "B").End(xlUp).Row
    ' Armazene os valores das colunas B e E em arrays
    valoresB = ws.Range("B1:B" & lastRow).Value
    valoresE = ws.Range("E1:E" & lastRow).Value
    
    ' Converta os valores da coluna B para maiúsculo
    For i = 1 To UBound(valoresB, 1)
        valoresB(i, 1) = UCase(valoresB(i, 1))
    Next i
    
    ' Feche o arquivo externo
    wb.Close False
End Sub

Sub LerValoresDoArquivoDeReferencia1(ByRef valoresB() As Variant, ByRef valoresE() As Variant)
    Dim outroArquivo As String
    Dim wb As Workbook
    Dim ws As Worksheet
    Dim lastRow As Long
    Dim i As Long
    
    ' Defina o caminho do arquivo externo
    outroArquivo = "Z:\Engenharia\lista-padrão-orçamento.xlsx"
    
    ' Verifique se o arquivo existe
    If Dir(outroArquivo) = "" Then
        MsgBox "O arquivo não foi encontrado."
        Exit Sub
    End If
    
    ' Abra o arquivo externo
    Set wb = Workbooks.Open(outroArquivo, ReadOnly:=True)
    
    ' Verifique se a planilha existe
    On Error Resume Next
    Set ws = wb.Sheets("Lista de preços")
    On Error GoTo 0
    
    If ws Is Nothing Then
        MsgBox "A planilha especificada não foi encontrada no arquivo."
        wb.Close False
        Exit Sub
    End If
    
    ' Encontre a última linha com dados na coluna B e na coluna E
    lastRow = ws.Cells(ws.Rows.Count, "B").End(xlUp).Row
    ' Armazene os valores das colunas B e E em arrays
    valoresB = ws.Range("B1:B" & lastRow).Value
    valoresE = ws.Range("E1:E" & lastRow).Value
    
    ' Converta os valores da coluna B para maiúsculo
    For i = 1 To UBound(valoresB, 1)
        valoresB(i, 1) = UCase(valoresB(i, 1))
    Next i
    
    ' Feche o arquivo externo
    wb.Close False
End Sub

Sub suporte_parede()
    Dim valoresB() As Variant
    Dim valoresE() As Variant
    Dim celulaAtiva As Range
    Dim i As Integer
    
    Application.ScreenUpdating = False ' Desativa atualizações de tela para acelerar o código
    
    ' Ler os valores do arquivo de referência
    LerValoresDoArquivoDeReferencia1 valoresB, valoresE
    
    ' Defina a célula ativa como o destino para a colagem
    Set celulaAtiva = ActiveCell
    
    ' Cole os valores das células B2 até B6 na coluna da célula ativa
    For i = 2 To 6
        celulaAtiva.Offset(i - 2, 0).Value = valoresB(i, 1)
    Next i
    
    ' Cole os valores das células E2 até E6 nas colunas à direita da célula ativa
    For i = 2 To 6
        celulaAtiva.Offset(i - 2, 1).Value = valoresE(i, 1)
    Next i
    
    Application.ScreenUpdating = True ' Ativa atualizações de tela novamente
End Sub
